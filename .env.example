# Environment Variables
# Copy this file to .env.local and update with your actual values

# Application
NEXT_PUBLIC_APP_NAME="Election 2026"
NEXT_PUBLIC_APP_URL="http://localhost:5500"
PORT=5500

# Database (example - replace with your actual database URL)
# DATABASE_URL="postgresql://username:password@localhost:5432/election2026"

# Admin API Key (required for Admin Dashboard write operations)
# ADMIN_API_KEY="changeme-dev-key"

# Admin seed secret (set this to enable /api/polls/seed in production)
# SEED_SECRET="set-a-strong-random-secret"

# Authentication (example - replace with your actual secret)
# NEXTAUTH_SECRET="your-secret-key-here"
# NEXTAUTH_URL="http://localhost:3000"

# API Keys (example - replace with your actual API keys)
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID="G-XXXXXXXXXX"
# EMAIL_SERVER_HOST="smtp.example.com"
# EMAIL_SERVER_PORT=587
# EMAIL_SERVER_USER="user@example.com"
# EMAIL_SERVER_PASSWORD="password"
# EMAIL_FROM="noreply@example.com"